  @keyframes edge-fade { 
  0% { --top-fade: 0; }
  3%, 100% { --top-fade: var(--edge-fade-distance,.5lh); }
  0%, 97% { --bottom-fade: var(--edge-fade-distance,.5lh); }
  100% { --bottom-fade: 0; }
